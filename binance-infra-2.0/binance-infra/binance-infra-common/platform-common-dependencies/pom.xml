<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://maven.apache.org/POM/4.0.0"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>platform-common-dependencies</artifactId>
	<version>2.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>platform-common-dependencies</name>
	<parent>
		<artifactId>binance-infra-dependencies-parent</artifactId>
		<groupId>com.binance.infra.avengers</groupId>
		<version>2.0.1-SNAPSHOT</version>
	</parent>
	<properties>
		<lombok.version>1.16.22</lombok.version>
		<commons-collections4.version>4.2</commons-collections4.version>
		<commons-lang3.version>3.8.1</commons-lang3.version>
		<commons.io.version>2.5</commons.io.version>
		<commons-net.version>3.6</commons-net.version>
		<skywalking-opentracing.version>6.4.0</skywalking-opentracing.version>
		<master.version>2.0.0-SNAPSHOT</master.version>
		<hibernate-validator.version>6.0.15.Final</hibernate-validator.version>
		<ip2location.version>1.0.1</ip2location.version>
		<swagger.version>2.0.1-SNAPSHOT</swagger.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.binance.infra.avengers</groupId>
				<artifactId>platform-common-core</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.projectlombok</groupId>
				<artifactId>lombok</artifactId>
				<version>${lombok.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.commons</groupId>
				<artifactId>commons-lang3</artifactId>
				<version>${commons-lang3.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.commons</groupId>
				<artifactId>commons-collections4</artifactId>
				<version>${commons-collections4.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-opentracing</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-logback-1.x</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-log4j-2.x</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-trace</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.hibernate</groupId>
				<artifactId>hibernate-validator</artifactId>
				<version>${hibernate-validator.version}</version>
			</dependency>
			<dependency>
				<groupId>com.binance.master</groupId>
				<artifactId>api-base</artifactId>
				<version>${master.version}</version>
				<exclusions>
					<exclusion>
						<groupId>io.github.openfeign</groupId>
						<artifactId>feign-jackson</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
			<dependency>
				<groupId>com.binance.master</groupId>
				<artifactId>common-lib</artifactId>
				<version>${master.version}</version>
			</dependency>
			<dependency>
				<groupId>com.ip2location</groupId>
				<artifactId>ip2location</artifactId>
				<version>${ip2location.version}</version>
			</dependency>
			<dependency>
				<groupId>commons-io</groupId>
				<artifactId>commons-io</artifactId>
				<version>${commons.io.version}</version>
			</dependency>
			<dependency>
				<groupId>commons-net</groupId>
				<artifactId>commons-net</artifactId>
				<version>${commons-net.version}</version>
			</dependency>
			<dependency>
				<groupId>com.binance.infra.avengers</groupId>
				<artifactId>platform-swagger-core</artifactId>
				<version>${swagger.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>

<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>platform-monitor-dependencies</artifactId>
	<version>2.0.1-SNAPSHOT</version>
	<name>platform-monitor-dependencies</name>
	<packaging>pom</packaging>
	<parent>
		<artifactId>binance-infra-dependencies-parent</artifactId>
		<groupId>com.binance.infra.avengers</groupId>
		<version>2.0.1-SNAPSHOT</version>
	</parent>
	<properties>
		<platform-common.version>2.0.1-SNAPSHOT</platform-common.version>
		<skywalking-opentracing.version>7.0.0</skywalking-opentracing.version>
		<logbackjson.version>0.1.5</logbackjson.version>
		<p6spy.version>3.8.0</p6spy.version>
		<jsqlparser.version>2.0</jsqlparser.version>
		<druid.version>1.1.17</druid.version>
		<sharding.version>3.1.3-SNAPSHOT</sharding.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.binance.infra.avengers</groupId>
				<artifactId>platform-common-dependencies</artifactId>
				<version>${platform-common.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>io.shardingsphere</groupId>
				<artifactId>sharding-jdbc-core</artifactId>
				<version>${sharding.version}</version>
				<optional>true</optional>
			</dependency>
			<dependency>
				<groupId>com.binance.infra.avengers</groupId>
				<artifactId>platform-monitor-core</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>com.binance.infra.avengers</groupId>
				<artifactId>platform-starter-monitor</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-opentracing</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-logback-1.x</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-log4j-2.x</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.skywalking</groupId>
				<artifactId>apm-toolkit-trace</artifactId>
				<version>${skywalking-opentracing.version}</version>
			</dependency>
			<!-- logback -->
			<dependency>
				<groupId>ch.qos.logback.contrib</groupId>
				<artifactId>logback-jackson</artifactId>
				<version>${logbackjson.version}</version>
			</dependency>
			<dependency>
				<groupId>ch.qos.logback.contrib</groupId>
				<artifactId>logback-json-classic</artifactId>
				<version>${logbackjson.version}</version>
			</dependency>
			<dependency>
				<groupId>p6spy</groupId>
				<artifactId>p6spy</artifactId>
				<version>${p6spy.version}</version>
			</dependency>
			<dependency>
				<groupId>com.github.jsqlparser</groupId>
				<artifactId>jsqlparser</artifactId>
				<version>${jsqlparser.version}</version>
			</dependency>
			<dependency>
				<groupId>com.alibaba</groupId>
				<artifactId>druid-spring-boot-starter</artifactId>
				<version>${druid.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
